{
  "name": "pakfiller",
  "version": "1.0.0",
  "description": "PakFiller - Tax Filing Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd be-filler && next dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd be-filler && next build",
    "start": "cd be-filler && next start",
    "deploy:frontend": "cd be-filler && vercel --prod",
    "deploy:backend": "cd backend && vercel --prod",
    "install:all": "npm install && cd be-filler && npm install && cd ../backend && npm install",
    "setup": "npm run install:all && echo 'Setup completed! Please configure environment variables.'"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.2.1",
    "concurrently": "^8.2.2",
    "style.css": "~1.0.3",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "tax-filing",
    "pakistan",
    "nextjs",
    "nodejs",
    "mongodb"
  ],
  "author": "PakFiller Team",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.6",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-i18next": "^15.4.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "shadcn": "^2.10.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  }
}
