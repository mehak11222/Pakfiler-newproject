{
  "name": "pakfiller",
  "version": "1.0.0",
  "description": "PakFiller - Tax Filing Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd be-filler && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd be-filler && npm run build",
    "build:backend": "cd backend && npm install",
    "deploy": "npm run deploy:backend && npm run deploy:frontend",
    "deploy:frontend": "cd be-filler && vercel --prod",
    "deploy:backend": "cd backend && vercel --prod",
    "install:all": "npm install && cd be-filler && npm install && cd ../backend && npm install",
    "setup": "npm run install:all && echo 'Setup completed! Please configure environment variables.'"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "tax-filing",
    "pakistan",
    "nextjs",
    "nodejs",
    "mongodb"
  ],
  "author": "PakFiller Team",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.12",
    "zod": "^4.0.17"
  }
}
